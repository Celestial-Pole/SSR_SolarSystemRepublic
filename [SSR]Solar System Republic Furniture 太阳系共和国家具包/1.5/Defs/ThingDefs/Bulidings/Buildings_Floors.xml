	<!-- 建筑——地毯 -->

	<ThingDef Abstract="True" Name="SSRBuildingFloorCoveringBase" ParentName="BuildingBase">
		<altitudeLayer>FloorCoverings</altitudeLayer>
		<wipesPlants>true</wipesPlants>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<selectable>true</selectable>
		<useHitPoints>true</useHitPoints>
		<defaultPlacingRot>South</defaultPlacingRot>
		<canGenerateDefaultDesignator>true</canGenerateDefaultDesignator>
		<blocksAltitudes>
			<li>FloorCoverings</li>
		</blocksAltitudes>
		<researchPrerequisites>
			<li>CarpetMaking</li>
		</researchPrerequisites>
		<statBases>
			<Beauty>30</Beauty>
			<WorkToBuild>2000</WorkToBuild>
			<Mass>150</Mass>
		</statBases>
		<building>
			<isEdifice>false</isEdifice>
		</building>
		<designationCategory>SSR_DesignationCategory</designationCategory>
		<modExtensions>
			<li Class="FS_SSR.UI.Ext_DesignationCategoryInner">
				<!-- SSR_DesignationCategory分类的必须带此ext注明子分类 -->
				<innerCategory>SSR_DesignationInnerCategory_Furniture</innerCategory>
				<designatorClass>Designator_Build</designatorClass>
			</li>
		</modExtensions>
	</ThingDef>

