		<constructionSkillPrerequisite>3</constructionSkillPrerequisite>
		<isPaintable>true</isPaintable>
		<researchPrerequisites>
			<li>CarpetMaking</li>
		</researchPrerequisites>
	</TerrainDef>

	<TerrainDef ParentName="SSRCarpetFloorBase">
		<defName>SSRCarpetI</defName>
		<label>原味地毯</label>
		<texturePath>Things/Building/Floor/SSRCarpetI</texturePath>
		<renderPrecedence>240</renderPrecedence>
		<description>原味地毯</description>
		<statBases>
			<WorkToBuild>800</WorkToBuild>
			<Beauty>0</Beauty>
			<Cleanliness>0.4</Cleanliness>
			<CleaningTimeFactor>0.4</CleaningTimeFactor>
		</statBases>
		<costList>
			<Cloth>7</Cloth>
