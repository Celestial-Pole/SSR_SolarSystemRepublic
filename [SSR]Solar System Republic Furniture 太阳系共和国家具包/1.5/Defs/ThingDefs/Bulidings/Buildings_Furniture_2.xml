<Defs>

	<!-- 地球仪 -->

	<ThingDef ParentName="SSR_BuildingBase_Furniture">
		<defName>SSR_Globe</defName>
		<label>地球仪</label>
		<description>地球仪</description>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<uiIconPath>Things/Building/Furniture/Globe/Globe</uiIconPath>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Mass>15</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>10</Beauty>
		</statBases>
		<costList>
			<Steel>50</Steel>
		</costList>
		<canOverlapZones>true</canOverlapZones>
		<pathCost>14</pathCost>
		<rotatable>false</rotatable>
		<size>(1,1)</size>
		<uiIconOffset>(0, 0.15)</uiIconOffset>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<minifiedDef>MinifiedThing</minifiedDef>
		<socialPropernessMatters>true</socialPropernessMatters>
		<fillPercent>0.35</fillPercent>
		<clearBuildingArea>false</clearBuildingArea>
		<building>
			<sowTag>Decorative</sowTag>
			<isEdifice>false</isEdifice>
			<destroySound>BuildingDestroyed_Soft_Medium</destroySound>
			<destroyShakeAmount>0</destroyShakeAmount>
		</building>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<placeWorkers>
			<li>SSR_Furniture.PlaceWorker_OnTable</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>ComplexFurniture</li>
		</researchPrerequisites>
		<graphicData>
			<drawSize>(1,1)</drawSize>
			<texPath>Things/Building/Furniture/Globe/Globe_Base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<comps>
			<li Class="FS_SSR.Generic.TCP_DrawOverlay">
				<overlay>
					<texPath>Things/Building/Furniture/Globe/Globe_Top</texPath>
					<graphicClass>Graphic_Random</graphicClass>
					<drawSize>(1,1)</drawSize>
				</overlay>
			</li>
		</comps>
	</ThingDef>

	<!-- 小电视 -->

	<ThingDef ParentName="SSR_BuildingBase_Furniture">
		<defName>SSR_Television_S</defName>
		<label>小电视</label>
		<description>小电视</description>
